import java.util.Scanner;

public class BMICalcBetter 
{
	public static void main( String[] args)
	{
		Scanner keyboard = new Scanner(System.in);
		
		int feet, inches, weight;
		
		System.out.println("BMI (Body Mass Index) is a way of representing your size.");
		
		System.out.println("How many feet tall are you?");
		feet = keyboard.nextInt();
		
		System.out.println("How many inches?");
		inches = keyboard.nextInt();
		
		System.out.println("How many pounds do you weigh?");
		weight = keyboard.nextInt();
		
		System.out.println("Your BMI is:" + (weight * .4536) / (((feet + inches/12) * .3048) * ((feet + inches/12) * .3048)));
		
		if((weight * .4536) / (((feet + inches/12) * .3048) * ((feet + inches/12) * .3048)) <= 18.5)
		{
			System.out.println("You are underweight");
		}
		
		if((weight * .4536) / (((feet + inches/12) * .3048) * ((feet + inches/12) * .3048)) >= 18.5 && (weight * .4536) / (((feet + inches/12) * .3048) * ((feet + inches/12) * .3048)) < 25)
		{
			System.out.println("You are normal weight");
		}
		
		if((weight * .4536) / (((feet + inches/12) * .3048) * ((feet + inches/12) * .3048)) >= 25 && (weight * .4536) / (((feet + inches/12) * .3048) * ((feet + inches/12) * .3048)) < 30)
		{
			System.out.println("You are overweight");
		}
		
		if((weight * .4536) / (((feet + inches/12) * .3048) * ((feet + inches/12) * .3048)) >= 30)
		{
			System.out.println("You are obese");
		}
		
		keyboard.close();
	}
}
