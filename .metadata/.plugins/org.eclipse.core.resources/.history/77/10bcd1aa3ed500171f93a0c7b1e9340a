
public class YourInitials 
{
	public static void main(String[] args)
	{
		System.out.println("CCCCC   TTTTT   PPPPP")
		System.out.println("C         T     P   P")
		System.out.println("C         T     P   P")
		System.out.println("C         T     PPPPP")
		System.out.println("C         T     P")
		System.out.println("CCCCC     T     P")
	}
}
